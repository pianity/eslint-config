        "eslint-plugin-react-hooks": "^4.5.0",
        "eslint-plugin-tsdoc": "^0.2.16"
    },
    "devDependencies": {
        "@pianity/prettier-config": "^0.0.0",
        "eslint": "^8.16.0",
        "prettier": "^2.6.2",
